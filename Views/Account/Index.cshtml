@using ShopMarket.Models
@model IEnumerable<ShopUser>
<div class="col">
    <table class="table table-dark">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(t=>t.First().Id)</th>
                <th>@Html.DisplayNameFor(t => t.First().UserName)</th>
                <th>@Html.DisplayNameFor(t => t.First().Email)</th>
                <th>@Html.DisplayNameFor(t => t.First().DateOfBirth)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td>@user.DateOfBirth</td>
                </tr>
            }
           
        </tbody>
    </table>
</div>
